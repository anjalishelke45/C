#include <stdio.h>


int n;
int adj[10][10]; int visited[10];

void DFS(int i) { int j;
printf("%d ", i); visited[i] = 1;

for (j = 0; j < n; j++) {
if (visited[j] == 0 && adj[i][j] == 1) { DFS(j);
}
}
}


void printMatrix() { int i, j;
printf("Adjacency Matrix:\n"); for (i = 0; i < n; i++) {
for (j = 0; j < n; j++) { printf("%d ", adj[i][j]);
}
printf("\n");
}
}


int main() { int i, j, s;
 
printf("Enter the total number of buildings: "); scanf("%d", &n);

printf("Enter the adjacency matrix:\n"); for (i = 0; i < n; i++) {
for (j = 0; j < n; j++) {
scanf("%d", &adj[i][j]);
}
}


printMatrix();


for (i = 0; i < n; i++) visited[i] = 0;


printf("Enter the starting building: "); scanf("%d", &s);

printf("DFS Traversal:\n"); DFS(s);
printf("\n");


return 0;
}
